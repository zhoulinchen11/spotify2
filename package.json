  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.0",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.16",
    "postcss": "^8.4.14"
  }
}
